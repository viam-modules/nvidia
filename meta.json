{
  "$schema": "https://dl.viam.dev/module.schema.json",
  "module_id": "viam:nvidia",
  "visibility": "public",
  "url": "https://github.com/viam-modules/nvidia",
  "description": "Go module for nvidia jetson board, compatible with Viam",
  "models": [
    {
      "api": "rdk:component:board",
      "model": "viam:nvidia:jetson",
      "markdown_link": "README.md#configure-your-jetson-board",
      "short_description": "board model for the nvidia jetson orin, jetson AGX, and jetson nano"
    }
  ],
  "build": {
    "build": "make module",
    "path": "bin/module.tar.gz",
    "arch" : ["linux/arm64"]
  },
  "entrypoint": "bin/nvidia"
}
